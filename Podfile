  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod "Texture"

  # Pods for Youtube-dl

  target 'Youtube-dlTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Youtube-dlUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

